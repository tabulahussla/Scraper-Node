{
	"node-id": false,
	"resource-broker": {
		"pools": {
			"proxy": ["appannie proxies", "google play", "app store"],
			"account": ["appannie accounts"]
		}
	},
	"queues": {
		"appannie.com": {
			"pools": ["appannie_proxies", "appannie_accounts"],
			"resources": ["proxy", "account"],
			"workerType": "agent",
			"concurrency": 25
		},
		"api.appannie.com": {
			"pools": ["appannie_proxies", "appannie_accounts"],
			"resources": ["proxy", "account"],
			"workerType": "http",
			"concurrency": 25
		},
		"play.google.com": {
			"pools": ["google_play_proxies"],
			"resources": ["proxy"],
			"workerType": "http",
			"concurrency": 25
		},
		"itunes.apple.com": {
			"pools": ["app_store_proxies"],
			"resources": ["proxy"],
			"workerType": "http",
			"concurrency": 25
		}
	},
	"plugins": [
		"@xxorg/google-play-plugin",
		"@xxorg/app-store-plugin",
		"@xxorg/app-annie-plugin"
	]
}
