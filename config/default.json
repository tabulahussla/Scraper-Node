{
	"project-name": "SCRAPER-NODE",
	"log-level": "debug",
	"node-id": "anonymous-scraper",
	"mongodb": {
		"url": "mongodb://localhost:27017/scraping",
		"options": {}
	},
	"redis": {
		"host": "127.0.0.1",
		"port": 6379,
		"db": 0
	},
	"api": {
		"port": 62111,
		"type": "ws",
		"tls": false
	},
	"jwt": {
		"algorithm": "HS384",
		"secret": "changeMe+++ThisISBase64EncodedToken==",
		"options": {}
	},
	"resource-broker": {
		"pools": {
			"proxy": [],
			"account": []
		},
		"token": ""
	},
	"discovery": {
		"service": "scraping-system",
		"payload": {
			"host": "localhost",
			"port": 62111
		},
		"client": {
			"type": "ws",
			"host": "5.189.189.195",
			"port": 30019,
			"token": "",
			"namespace": "default"
		}
	},
	"userAgent": "AAppC34JTB1NDTWD:290",
	"make-folders": ["storage/profiles"],
	"profileDir": "storage/profiles",
	"cluster": {
		"enabled": false,
		"maxWorkers": 4,
		"entryPointForWorker": "src/worker.index.js"
	},
	"queues": {
		"appannie.com": {
			"pools": ["appannie-proxies", "appannie-accounts"],
			"resources": ["proxy", "account"],
			"workerType": "agent",
			"concurrency": 25
		},
		"api.appannie.com": {
			"pools": ["appannie-proxies", "appannie-accounts"],
			"resources": ["proxy", "account"],
			"workerType": "http",
			"concurrency": 25
		},
		"play.google.com": {
			"pools": ["google-play-proxies"],
			"resources": ["proxy"],
			"workerType": "http",
			"concurrency": 25
		},
		"itunes.apple.com": {
			"pools": ["app-store-proxies"],
			"resources": ["proxy"],
			"workerType": "http",
			"concurrency": 25
		}
	},
	"puppeteer": {
		"headless": true,
		"timeout": 120000
	},
	"agent": {
		"defaultLanguages": ["en-US", "en"],
		"defaultNavigationTimeout": 125000,
		"destroyOnNetworkError": true,
		"destroyAfterTimeout": 300000,
		"warnIfNotReturnedTimeout": 800000
	},
	"plugins": ["@xxorg/google-play-plugin", "@xxorg/app-store-plugin", "@xxorg/app-annie-plugin"]
}
