{
	"project-name": "SCRAPER-NODE",
	"log-level": "trace",
	"mongodb": {
		"url": "mongodb://localhost:27017/scraping",
		"options": {}
	},
	"redis": {
		"host": "127.0.0.1",
		"port": 6379,
		"db": 0
	},
	"service": {
		"host": "127.0.0.1",
		"port": 62111,
		"tlsOptions": {
			"sessionTimeout": 15000,
			"rejectUnauthorized": true,
			"requestCert": true,
			"ca": "./ROOT.crt",
			"key": "./SERVER.key",
			"cert": "./SERVER.crt"
		}
	},
	"resource-broker": {
		"pools": {
			"proxy": ["appannie proxies", "google play", "app store"],
			"account": ["appannie accounts"]
		},
		"host": "127.0.0.1",
		"port": 62137,
		"tlsOptions": {
			"sessionTimeout": 15000,
			"rejectUnauthorized": true,
			"requestCert": true,
			"ca": "../resource-broker/ROOT.crt",
			"key": "../resource-broker/SERVER.key",
			"cert": "../resource-broker/SERVER.crt"
		}
	},
	"make-folders": ["storage/profiles"],
	"profileDir": "storage/profiles",
	"cluster": {
		"enabled": false,
		"maxWorkers": 4,
		"entryPointForWorker": "src/worker.index.js"
	},
	"queues": {
		"appannie-web": {
			"pools": ["appannie proxies", "appannie accounts"],
			"resources": ["proxy", "account"],
			"workerType": "agent",
			"concurrency": 25
		},
		"appannie-http": {
			"pools": ["appannie proxies", "appannie accounts"],
			"resources": ["proxy", "account"],
			"workerType": "http",
			"concurrency": 25
		},
		"google-play": {
			"pools": ["google play"],
			"resources": ["proxy"],
			"workerType": "http",
			"concurrency": 25
		},
		"app-store": {
			"pools": ["app store"],
			"resources": ["proxy"],
			"workerType": "http",
			"concurrency": 25
		}
	},
	"puppeteer": {
		"headless": true,
		"timeout": 120000
	},
	"agent": {
		"defaultLanguages": ["en-US", "en"],
		"defaultNavigationTimeout": 125000,
		"destroyOnNetworkError": true,
		"destroyAfterTimeout": 300000,
		"warnIfNotReturnedTimeout": 800000
	},
	"service-discovery": {
		"id": "default",
		"host": "127.0.0.1"
	}
}
