{
	"project-name": "SCRAPER-NODE",
	"log-level": "trace",
	"mongodb": {
		"url": "mongodb://localhost:27017/scraping",
		"options": {}
	},
	"redis": {},
	"make-folders": ["storage/profiles"],
	"profileDir": "storage/profiles",
	"resource-broker": {},
	"cluster": {
		"enabled": false,
		"maxWorkers": 4,
		"entryPointForWorker": "src/worker.index.js"
	},
	"queues": {
		"appannie-web": {
			"resources": ["proxy", "account"],
			"workerType": "agent",
			"concurrency": 25
		},
		"appannie-http": {
			"resources": ["proxy", "account"],
			"workerType": "http",
			"concurrency": 25
		},
		"google-play": {
			"resources": ["proxy"],
			"workerType": "http",
			"concurrency": 25
		},
		"app-store": {
			"resources": ["proxy"],
			"workerType": "http",
			"concurrency": 25
		}
	},
	"puppeteer": {
		"headless": true,
		"timeout": 120000
	},
	"agent": {
		"defaultLanguages": ["en-US", "en"],
		"defaultNavigationTimeout": 125000
	}
}
