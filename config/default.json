{
	"project-name": "SCRAPER-NODE",
	"log-level": "trace",
	"node-id": "anonymous-scraper",
	"mongodb": {
		"url": "mongodb://localhost:27017/scraping",
		"options": {}
	},
	"redis": {
		"host": "127.0.0.1",
		"port": 6379,
		"db": 0
	},
	"api": {
		"port": 62111,
		"type": "ws",
		"tls": false
	},
	"jwt": {
		"algorithm": "HS384",
		"secret": "changeMe+++ThisISBase64EncodedToken==",
		"options": {}
	},
	"resource-broker": {
		"pools": {
			"proxy": ["appannie proxies", "google play", "app store"],
			"account": ["appannie accounts"]
		},
		"token": ""
	},
	"discovery": {
		"service": "scraping-system",
		"payload": {
			"host": "localhost",
			"port": 62111
		},
		"client": {
			"type": "ws",
			"host": "5.189.189.195",
			"port": 30019,
			"token": "",
			"namespace": "default"
		}
	},
	"make-folders": ["storage/profiles"],
	"profileDir": "storage/profiles",
	"cluster": {
		"enabled": false,
		"maxWorkers": 4,
		"entryPointForWorker": "src/worker.index.js"
	},
	"queues": {
		"appannie-web": {
			"pools": ["appannie proxies", "appannie accounts"],
			"resources": ["proxy", "account"],
			"workerType": "agent",
			"concurrency": 25
		},
		"appannie-http": {
			"pools": ["appannie proxies", "appannie accounts"],
			"resources": ["proxy", "account"],
			"workerType": "http",
			"concurrency": 25
		},
		"google-play": {
			"pools": ["google play"],
			"resources": ["proxy"],
			"workerType": "http",
			"concurrency": 25
		},
		"app-store": {
			"pools": ["app store"],
			"resources": ["proxy"],
			"workerType": "http",
			"concurrency": 25
		}
	},
	"puppeteer": {
		"headless": true,
		"timeout": 120000
	},
	"agent": {
		"defaultLanguages": ["en-US", "en"],
		"defaultNavigationTimeout": 125000,
		"destroyOnNetworkError": true,
		"destroyAfterTimeout": 300000,
		"warnIfNotReturnedTimeout": 800000
	},
	"plugins": ["google-play-scraping"]
}
