{
	"project-name": "SCRAPER-NODE",
	"log-level": "trace",
	"mongodb": {
		"url": "mongodb://localhost:27017/scraping",
		"options": {}
	},
	"redis": {},
	"make-folders": ["storage/profiles"],
	"profileDir": "storage/profiles",
	"resource-broker": {
		"host": "127.0.0.1",
		"port": 62137,
		"ca": "../resource-broker/ROOT.crt",
		"key": "../resource-broker/SERVER.key",
		"cert": "../resource-broker/SERVER.crt",
		"pools": {
			"proxy": ["test proxies"],
			"account": ["test accounts"]
		}
	},
	"cluster": {
		"enabled": false,
		"maxWorkers": 4,
		"entryPointForWorker": "src/worker.index.js"
	},
	"queues": {
		"test-agent": {
			"pools": ["test accounts"],
			"resources": ["account"],
			"workerType": "agent",
			"concurrency": 25
		},
		"test-http": {
			"pools": ["test proxies", "test accounts"],
			"resources": ["proxy", "account"],
			"workerType": "http",
			"concurrency": 25
		}
	},
	"puppeteer": {
		"headless": true,
		"timeout": 120000
	},
	"agent": {
		"defaultLanguages": ["en-US", "en"],
		"defaultNavigationTimeout": 125000,
		"destroyOnNetworkError": true,
		"destroyAfterTimeout": 300000,
		"warnIfNotReturnedTimeout": 800000
	}
}
