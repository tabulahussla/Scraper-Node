{
	"name": "@xxorg/scraper-node",
	"version": "1.0.0",
	"description": "Node.js scraping system",
	"main": "src/index.js",
	"repository": "git@gitlab.com:index_php/scraping/scraper-node.git",
	"author": "Georgy Chirkov <mail@api.haus>",
	"license": "UNLICENSED",
	"private": true,
	"engines": {
		"node": ">= 8.2.0"
	},
	"scripts": {
		"start": "NODE_ENV=production node dist/",
		"validate-token": "babel-node bin/token/validate",
		"generate-token": "babel-node bin/token/generate",
		"lint": "eslint --fix src/. test/. --config .eslintrc.json",
		"test": "NODE_ENV=test mocha --bail --exit --recursive --timeout 10000 -r @babel/register -r @babel/polyfill",
		"cov": "NODE_ENV=test nyc mocha --bail --exit --recursive -r @babel/register -r @babel/polyfill",
		"tdd": "yarn run test -w --reporter min",
		"dev": "nodemon bin/nodemon --exec babel-node",
		"pm2": "pm2 start process.yml --env production",
		"build": "rm -rf dist; babel src/ -d dist/",
		"uglify": "uglifyjs dist/bundle.min.js -o dist/bundle.min.js --compress --mangle toplevel",
		"obfuscate": "javascript-obfuscator dist/bundle.min.js -o dist/bundle.min.js --config obfuscate.config.json"
	},
	"dependencies": {
		"@xxorg/maintenance-common": "latest",
		"@xxorg/resource-broker-client": "latest",
		"@xxorg/scraping-plugins": "^0.5.1",
		"amqplib": "^0.5.3",
		"bluebird": "^3.5.3",
		"clone": "^2.1.2",
		"config": "^3.0.1",
		"fs-extra": "^7.0.0",
		"ioredis": "^4.6.2",
		"measured-core": "^1.40.2",
		"mongodb": "^3.1.13",
		"pino": "^5.12.2",
		"proxy-agent": "^3.0.3",
		"puppeteer": "^1.12.2",
		"redis": "^2.8.0",
		"request": "^2.88.0",
		"request-promise-native": "^1.0.5"
	},
	"devDependencies": {
		"@babel/cli": "^7.0.0-beta.55",
		"@babel/core": "^7.0.0-rc.2",
		"@babel/node": "^7.0.0-beta.55",
		"@babel/preset-env": "^7.3.1",
		"@babel/register": "^7.0.0-beta.55",
		"@types/amqplib": "^0.5.11",
		"@types/config": "^0.0.34",
		"@types/fs-extra": "^5.0.4",
		"@types/mocha": "^5.2.5",
		"@types/mongodb": "^3.1.12",
		"@types/node": "^10.12.21",
		"@types/pino": "^5.8.6",
		"@types/puppeteer": "^1.12.1",
		"@types/request": "^2.48.1",
		"@types/request-promise-native": "^1.0.15",
		"babel-plugin-add-module-exports": "^1.0.0",
		"babel-plugin-module-resolver": "^3.1.3",
		"eslint": "^5.13.0",
		"eslint-import-resolver-babel-module": "^5.0.1",
		"eslint-plugin-babel": "^5.1.0",
		"eslint-plugin-import": "^2.16.0",
		"eslint-plugin-node": "^8.0.1",
		"javascript-obfuscator": "^0.18.1",
		"mocha": "^5.2.0",
		"nodemon": "^1.18.3",
		"nyc": "^13.2.0",
		"pino-pretty": "^2.6.0",
		"rollup": "^1.1.2",
		"rollup-plugin-babel": "^4.0.3",
		"rollup-plugin-commonjs": "^9.2.0",
		"rollup-plugin-json": "^3.1.0",
		"rollup-plugin-node-resolve": "^4.0.0",
		"uglify-es": "^3.3.9"
	}
}
